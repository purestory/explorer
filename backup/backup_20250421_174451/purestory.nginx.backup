server { listen 80; server_name itsmyzone.iptime.org; client_max_body_size 50G; client_body_timeout 3600s; proxy_connect_timeout 3600s; proxy_send_timeout 3600s; proxy_read_timeout 3600s; location = /webdav { rewrite ^ /webdav/ permanent; } location /webdav/ { alias /home/purestory/webdav/frontend/; index index.html; } location /webdav-api/ { proxy_pass http://[::1]:3333/webdav-api/; proxy_set_header Host $host; proxy_set_header X-Real-IP $remote_addr; proxy_buffering off; client_max_body_size 50G; } location = /favicon.ico { alias /home/purestory/webdav/frontend/favicon.ico; } types { application/javascript js; text/html html; text/css css; } }
